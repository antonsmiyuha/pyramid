#!/var/www/piramid/env/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'feedback','console_scripts','initialize_feedback_db'
__requires__ = 'feedback'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('feedback', 'console_scripts', 'initialize_feedback_db')()
    )
